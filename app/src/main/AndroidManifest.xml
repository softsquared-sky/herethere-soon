<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.herethereproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:screenOrientation="portrait"
        android:theme="@style/Theme.AppCompat.NoActionBar">

        <activity android:name="com.example.template.src.login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.template.src.signUp.SignUpEmailActivity"></activity>
        <activity android:name="com.example.template.src.signUp.SignUpPasswordActivity"></activity>
        <activity android:name="com.example.template.src.signUp.SignUpPrivacyActivity"></activity>
        <activity android:name="com.example.template.src.signUp.SignUpPictureActivity"></activity>
    </application>

</manifest>